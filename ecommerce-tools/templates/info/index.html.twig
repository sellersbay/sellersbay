{% extends 'base.html.twig' %}

{% block title %}System Information{% endblock %}

{% block body %}
    <div class="container mt-4">
        <h1>System Information</h1>
        <div class="card">
            <div class="card-body">
                <table class="table">
                    <tbody>
                    {% for key, value in systemInfo %}
                        <tr>
                            <th>{{ key }}</th>
                            <td>{{ value }}</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
{% endblock %}